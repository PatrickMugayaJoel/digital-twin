{
  "env": {
    "node": true
  },
  "parser": "babel-eslint",
  "extends": ["react-app", "airbnb-base", "plugin:security/recommended", "plugin:prettier/recommended"],
  "plugins": ["security", "prettier", "react"],
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {
    "no-console": "warn",
    "func-names": "off",
    "no-unused-vars": "off",
    "no-underscore-dangle": "off",
    "consistent-return": "off",
    "security/detect-object-injection": "off"
  }
}
